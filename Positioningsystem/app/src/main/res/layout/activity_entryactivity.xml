<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="0dp"
    android:paddingLeft="0dp"
    android:paddingRight="0dp"
    android:paddingTop="0dp"
    tools:context="diogoferreira.positioningsystem.Entryactivity">

    <ImageView
        android:id="@+id/itemslist"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_below="@+id/imageView2"
        android:layout_marginStart="22dp"
        android:layout_marginTop="59dp"
        android:layout_toEndOf="@+id/textView"
        android:src="@color/bright_foreground_disabled_material_light"
        app:srcCompat="@drawable/items_v2" />

    <ImageView
        android:id="@+id/maps"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:src="@color/bright_foreground_disabled_material_light"
        app:srcCompat="@drawable/map_v3"
        android:layout_marginTop="24dp"
        android:layout_below="@+id/itemslist"
        android:layout_alignEnd="@+id/itemslist" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="160dp"
        android:layout_height="160dp"
        app:srcCompat="@drawable/inescid"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="View Items"
        android:textSize="30sp"
        android:layout_below="@+id/imageView2"
        android:layout_alignParentStart="true"
        android:layout_marginStart="38dp"
        android:layout_marginTop="78dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="View Map"
        android:textSize="30sp"
        android:layout_alignBottom="@+id/maps"
        android:layout_alignStart="@+id/textView"
        android:layout_marginBottom="26dp" />
</RelativeLayout>
